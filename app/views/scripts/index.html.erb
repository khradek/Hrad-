<h1>Listing scripts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Play1</th>
      <th>Hash1</th>
      <th>Down1</th>
      <th>Distance1</th>
      <th>Defense1</th>
      <th>Sed</th>
      <th>Situation1</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scripts.each do |script| %>
      <tr>
        <td><%= script.name %></td>
        <td><%= script.play1 %></td>
        <td><%= script.hash1 %></td>
        <td><%= script.down1 %></td>
        <td><%= script.distance1 %></td>
        <td><%= script.defense1 %></td>
        <td><%= script.sed_id %></td>
        <td><%= script.situation1 %></td>
        <td><%= link_to 'Show', script %></td>
        <td><%= link_to 'Edit', edit_script_path(script) %></td>
        <td><%= link_to 'Destroy', script, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Script', new_script_path %>
