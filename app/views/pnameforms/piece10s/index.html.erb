<h1><%= @pnameform.pname10 %>s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @piece10s.each do |piece10| %>
      <tr>
        <td><%= piece10.name %></td>
        <td><%= link_to 'Edit', edit_pnameform_piece10_path(@pnameform, piece10) %></td>
        <td><%= link_to 'Destroy', [@pnameform, piece10], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "New #{@pnameform.pname10}", new_pnameform_piece10_path %>
<%= link_to 'Back', @pnameform %>
