<h1><%= @pnameform.pname5 %>s</h1>

<div class="media">
  <div class="media-body">
    <table class="table table hover">
    <thead>
      <tr>
        <th>Name</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @piece5s.each do |piece5| %>
        <tr>
          <td><strong><%= piece5.name %></strong></td>
          <td><%= link_to 'Edit', edit_pnameform_piece5_path(@pnameform, piece5) %></td>
          <td><%= link_to 'Delete', [@pnameform, piece5], method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
    </table>
  </div>  
</div> 
<br>

<%= link_to "New#{@pnameform.pname5}", new_pnameform_piece5_path %>
<%= link_to 'Back', @pnameform %>
