<h1><%= @pnameform.pname1 %>s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @piece1s.each do |piece1| %>
      <tr>
        <td><%= piece1.name %></td>
        <td><%= link_to 'Edit', edit_pnameform_piece1_path(@pnameform, piece1) %></td>
        <td><%= link_to 'Destroy', [@pnameform, piece1], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "New #{@pnameform.pname1}" , new_pnameform_piece1_path %>
<%= link_to 'Back', @pnameform %>
