<h1><%= @pnameform.pname3 %>s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @piece3s.each do |piece3| %>
      <tr>
        <td><%= piece3.name %></td>
        <td><%= link_to 'Edit', edit_pnameform_piece3_path(@pnameform, piece3) %></td>
        <td><%= link_to 'Destroy', [@pnameform, piece3], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "New #{@pnameform.pname3}", new_pnameform_piece3_path %>
<%= link_to 'Back', @pnameform %>
