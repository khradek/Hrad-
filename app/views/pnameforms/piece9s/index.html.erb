<h1><%= @pnameform.pname9 %>s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @piece9s.each do |piece9| %>
      <tr>
        <td><%= piece9.name %></td>
        <td><%= link_to 'Edit', edit_pnameform_piece9_path(@pnameform, piece9) %></td>
        <td><%= link_to 'Destroy', [@pnameform, piece9], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "New #{@pnameform.pname9}", new_pnameform_piece9_path %>
