<h1><%= @pnameform.pname14 %>s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @piece14s.each do |piece14| %>
      <tr>
        <td><%= piece14.name %></td>
        <td><%= link_to 'Edit', edit_pnameform_piece14_path(@pnameform, piece14) %></td>
        <td><%= link_to 'Destroy', [@pnameform, piece14], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "New #{@pnameform.pname14}", new_pnameform_piece14_path %>
<%= link_to 'Back', @pnameform %>
