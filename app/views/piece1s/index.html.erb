<h1>Listing piece1s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @piece1s.each do |piece1| %>
      <tr>
        <td><%= piece1.name %></td>
        <td><%= piece1.user_id %></td>
        <td><%= link_to 'Show', piece1 %></td>
        <td><%= link_to 'Edit', edit_piece1_path(piece1) %></td>
        <td><%= link_to 'Destroy', piece1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Piece1', new_piece1_path %>
