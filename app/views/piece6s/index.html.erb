<h1>Listing piece6s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @piece6s.each do |piece6| %>
      <tr>
        <td><%= piece6.name %></td>
        <td><%= piece6.user_id %></td>
        <td><%= link_to 'Show', piece6 %></td>
        <td><%= link_to 'Edit', edit_piece6_path(piece6) %></td>
        <td><%= link_to 'Destroy', piece6, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Piece6', new_piece6_path %>
