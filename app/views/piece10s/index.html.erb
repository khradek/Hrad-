<h1>Listing piece10s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @piece10s.each do |piece10| %>
      <tr>
        <td><%= piece10.name %></td>
        <td><%= piece10.user_id %></td>
        <td><%= link_to 'Show', piece10 %></td>
        <td><%= link_to 'Edit', edit_piece10_path(piece10) %></td>
        <td><%= link_to 'Destroy', piece10, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Piece10', new_piece10_path %>
