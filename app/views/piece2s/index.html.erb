<h1>Listing piece2s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @piece2s.each do |piece2| %>
      <tr>
        <td><%= piece2.name %></td>
        <td><%= piece2.user_id %></td>
        <td><%= link_to 'Show', piece2 %></td>
        <td><%= link_to 'Edit', edit_piece2_path(piece2) %></td>
        <td><%= link_to 'Destroy', piece2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Piece2', new_piece2_path %>
