<h1>Listing piece5s</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @piece5s.each do |piece5| %>
      <tr>
        <td><%= piece5.name %></td>
        <td><%= piece5.user_id %></td>
        <td><%= link_to 'Show', piece5 %></td>
        <td><%= link_to 'Edit', edit_piece5_path(piece5) %></td>
        <td><%= link_to 'Destroy', piece5, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Piece5', new_piece5_path %>
